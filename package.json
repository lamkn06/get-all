  "scripts": {
    "build": "cross-env NODE_ENV=production yarn build:app && yarn build:server",
    "build:analyzer": "cross-env NODE_ENV=production webpack --config ./webpack/webpack.analyzer.js",
    "build:app": "webpack --config webpack/webpack.prod.js",
    "build:server": "webpack --config webpack/webpack.server.js",
    "server": "cross-env PORT=8080 nodemon server.js",
    "dev": "cross-env webpack serve --config ./webpack/webpack.dev.js",
    "start": "cross-env NODE_ENV=production nodemon build/server.*.js",
    "test": "jest --testPathPattern=src/",
    "eject": "react-scripts eject",
    "prettier": "prettier -c --write \"src/**/*\"",
    "lint:script:update": "eslint ./ --ext .ts --ext .tsx --color --fix",
    "lint:style:update": "stylelint ./src/**/*.{css,scss} --fix",
    "prepare": "husky install"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --cache --fix"
    ]
  }
